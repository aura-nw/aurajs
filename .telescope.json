{
  "protoDirs": [
    "proto",
    "cosmos-proto/proto",
    "cosmos-sdk-v0.45.11/proto",
    "ibc-go-v3.3.0/proto",
    "wasmd-v0.29.1/proto",
    "ics23/proto",
    "tendermint/proto",
    "aurad-v0.4.3/proto"
  ],
  "outPath": "./src/codegen",
  "options": {
    "aminoEncoding": {
      "enabled": true
    },
    "lcdClients": {
      "enabled": true
    },
    "rpcClients": {
      "enabled": true
    },
    "prototypes": {
      "typingsFormat": {
        "timestamp": "date",
        "duration": "string"
      },
      "methods": {
        "fromJSON": true,
        "toJSON": true,
        "encode": true,
        "decode": true,
        "fromPartial": true,
        "toAmino": false,
        "fromAmino": false,
        "fromProto": false,
        "toProto": false
      }
    }
  }
}